{
  "name": "priconne-quest-helper",
  "version": "0.0.1",
  "scripts": {
    "dev": "set NODE_ENV=development&& npm run dev:vite -- --open",
    "dev:host": "set NODE_ENV=development&& npm run dev:vite -- --host",
    "dev:vite": "vite dev",
    "build": "npm run clean && vite build",
    "deploy": "gh-pages -d build",
    "package": "svelte-kit package",
    "preview": "vite preview --host",
    "prepare": "npm run smui-theme",
    "smui-theme": "smui-theme compile src/smui.css -i src/theme",
    "check": "svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:watch": "npm test -- --watch",
    "clean": "trash build",
    "db_update": "node .github/workflows/pqh-updater/update.cjs"
  },
  "devDependencies": {
    "@actions/core": "^1.10.0",
    "@smui-extra/accordion": "^7.0.0-beta.0",
    "@smui/button": "^7.0.0-beta.0",
    "@smui/card": "^6.0.0",
    "@smui/checkbox": "^7.0.0-beta.0",
    "@smui/chips": "^7.0.0-beta.0",
    "@smui/data-table": "^7.0.0-beta.0",
    "@smui/dialog": "^7.0.0-beta.0",
    "@smui/form-field": "^7.0.0-beta.0",
    "@smui/icon-button": "^6.1.4",
    "@smui/linear-progress": "^7.0.0-beta.0",
    "@smui/list": "^7.0.0-beta.0",
    "@smui/menu": "^6.2.0",
    "@smui/menu-surface": "^7.0.0-beta.0",
    "@smui/paper": "^7.0.0-beta.0",
    "@smui/segmented-button": "^7.0.0-beta.0",
    "@smui/select": "^7.0.0-beta.0",
    "@smui/slider": "^7.0.0-beta.0",
    "@smui/switch": "^7.0.0-beta.0",
    "@smui/tab": "^7.0.0-beta.0",
    "@smui/tab-bar": "^7.0.0-beta.0",
    "@smui/textfield": "^7.0.0-beta.0",
    "@smui/tooltip": "^7.0.0-beta.0",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-static": "^1.0.0",
    "@sveltejs/kit": "^1.0.0",
    "@testing-library/svelte": "^3.1.3",
    "@types/cookie": "^0.5.1",
    "autoprefixer": "^10.4.8",
    "c8": "^7.12.0",
    "gh-pages": "^4.0.0",
    "jest-environment-jsdom": "^28.1.3",
    "jsdom": "^20.0.0",
    "postcss": "^8.4.14",
    "python-shell": "^3.0.1",
    "smui-theme": "^6.0.0",
    "sqlite": "^4.1.2",
    "sqlite3": "^5.1.2",
    "svelte": "^3.46.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.7",
    "trash-cli": "^5.0.0",
    "typescript": "^4.7.4",
    "vite": "^4.0.0",
    "vitest": "^0.19.1"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/fira-mono": "^4.5.0",
    "cookie": "^0.4.1",
    "webp-converter": "^2.3.3"
  }
}
